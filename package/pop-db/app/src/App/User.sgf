<?php

namespace {{app}};

class User extends \Strukt\Db\Type\Pop\Contract\Entity{

	/**
	* @Type(string)
	*/
	public  $username;

	/**
	* @Type(string)
	*/
	public  $password;

	/**
	* @Type(string)
	*/
	public  $status;

	/**
	* @Type(int)
	*/
	public  $role_id;

	/**
	* @Type(datetime)
	* @Default(now)
	*/
	public  $created_at;

	/**
	* @return Payroll\Role
	*/
	public function role(){

		return $this->belongsTo(Role::class, "role_id");
	}
}